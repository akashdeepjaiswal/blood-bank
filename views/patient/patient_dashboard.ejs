<%- include('header',{title: "Patient Dashboard" }); -%>

    <div class="container dashboard-content">

        <div class="dashbord-left">
            <img src="../images/patient.jpg">
        </div>

        <div class="dashbord-right table-parent">
            <table class="table">
                <thead>
                    <tr>
                        <th>Feilds</th>
                        <th>Your Details</th>
                    </tr>
                </thead>
                <tbody>
                    <tr>
                        <td>ID</td>
                        <td>
                            <%= id %>
                        </td>
                    </tr>
                    <tr>
                        <td>Name</td>
                        <td>
                            <%= name %>
                        </td>
                    </tr>
                    <tr>
                        <td>Email</td>
                        <td>
                            <%= email %>
                        </td>
                    </tr>
                    <tr>
                        <td>Password</td>
                        <td>
                            <%= password %>
                        </td>
                    </tr>
                    <tr>
                        <td>Contact</td>
                        <td>
                            <%= contact %>
                        </td>
                    </tr>
                    <tr>
                        <td>Blood Group</td>
                        <td>
                            <%= blood_group %>
                        </td>
                    </tr>

                </tbody>
            </table>
        </div>
        </div>
        <%- include('footer'); -%>